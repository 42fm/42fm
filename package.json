{
  "name": "42fm",
  "version": "0.1.5",
  "scripts": {
    "build": "yarn build:chromium:prod && yarn build:firefox:prod",
    "start:chromium": "webpack --env=DIST=chromium --env=BROWSER=chrome --watch --config webpack.dev.js",
    "start:firefox": "webpack --env=DIST=firefox --env=BROWSER=firefox --watch --config webpack.dev.js",
    "build:chromium:prod": "webpack --mode production --env=DIST=chromium --env=BROWSER=chrome --config webpack.prod.js",
    "build:firefox:prod": "webpack --mode production --env=DIST=firefox --env=BROWSER=firefox --config webpack.prod.js",
    "build:chromium:dev": "webpack --env=DIST=chromium --env=BROWSER=chrome --config webpack.dev.js",
    "build:firefox:dev": "webpack --env=DIST=firefox --env=BROWSER=firefox --config webpack.dev.js",
    "package": "(cd dist/chromium && zip -r ../42fm_chromium_$(jq -r .version ../../package.json).zip *) && (cd dist/firefox && zip -r ../42fm_firefox_$(jq -r .version ../../package.json).zip *)"
  },
  "author": "loczek",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/react": "^0.26.0",
    "@iconscout/react-unicons": "^2.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "date-fns": "^2.28.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.0",
    "react-merge-refs": "^2.1.1",
    "socket.io-client": "^4.4.1",
    "styled-components": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "vite": "^4.1.3",
    "webextension-polyfill": "^0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@hot-loader/react-dom": "^17.0.2",
    "@types/chrome": "^0.0.235",
    "@types/node": "^18.14.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.23",
    "@types/styled-components": "^5.1.20",
    "@types/webextension-polyfill": "^0.8.2",
    "@types/youtube": "^0.0.47",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.6.2",
    "val-loader": "^5.0.1",
    "vite-plugin-static-copy": "^0.13.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "packageManager": "yarn@4.1.1"
}
